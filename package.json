{
  "name": "static-api-generator",
  "version": "1.0.0",
  "description": "Build and deploy a static API using Node, TypeScript, eslint, prettier and vitest",
  "type": "module",
  "main": "./build/main.js",
  "exports": "./build/main.js",
  "types": "./build/main.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check": "npm run format && npm run lint && npm run test",
    "dev": "tsx ./src/import.ts",
    "format": "prettier . --write",
    "lint": "eslint .",
    "start": "node build/main.js",
    "test": "vitest run ./tests/unit",
    "test:integration": "playwright test",
    "test:watch": "vitest ./tests/unit"
  },
  "author": "kmturley",
  "license": "MIT",
  "keywords": [
    "static",
    "api",
    "generator"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.40.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "eslint": "^9.12.0",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "tsx": "^4.10.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vitest": "^1.6.1",
    "zod": "^4.0.15"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.3",
    "crawlee": "^3.14.1",
    "csv": "^6.4.1",
    "glob": "^11.0.3",
    "js-yaml": "^4.1.0",
    "slugify": "^1.6.6",
    "xml-js": "^1.6.11"
  }
}
